package ex01;

public class Hello {
	public static void main(String[] args) {

		int kor = 10;   //변수 선언
	
		
		kor = kor + 20;
		
		kor = kor + 100;
		
		System.out.println(kor);
		
		
	   double f = 1.2;
	
	   float f2 = 1.2f;
	   
	   long large = 10;
		
	   int i = (int)large;
	   
	   char a = '\uac22';
	   char b  = 88;
	   char c = 'D';
	   
	   
	   
	   System.out.println(a);
	   System.out.println(b);
	   System.out.println(c);
	   
	   boolean flag = true;   //거짓 : false, 0, 0.0   //참 : true, 정수는 0외 값, 실수 0.0외 값
	   
	   if(true)
		   System.out.println("참일 때 실행");
	   else
		   System.out.println("거짓 일때 실행");
	   
	   
	   String s1 = "Java";     //변수 외...........첫글자 대문자이면 클래스
	   String s2 = "Study";
	   
	   System.out.println(s1 + " " + s2);
	   
	
	}
}










