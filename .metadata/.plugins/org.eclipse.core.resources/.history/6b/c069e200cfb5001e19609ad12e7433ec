package ex01;

public class InputOutput01 {

	public static void main(String[] args) {
		
		String myName = "홍길동";
		int myAge = 20;
		
		System.out.println("이름 : \n" + myName);
		System.out.println("나이:\t" + myAge);
		System.out.println("JavaStudy\'s");
		

		System.out.println("나누기 : " + 5 / 3);
		System.out.println("나머지 : " + 5 % 3);
		
		
		if(5>3 && 5>12)
			System.out.println("참");
		else
			System.out.println("거짓");

		if(5>3 || 5>12)
			System.out.println("참");
		else
			System.out.println("거짓");
		
		
		int su1 = 5;
		int su2 = 6;   //  
		
		/* 
		
		
		
		*/
		
		System.out.println("수1 : " + ( su1 & su2 ) );
		System.out.println("수2 : " + (su1 | su2));
			
	
		float b2 = 129; //자동형변환  int정수 129를 자바가 129를 float형으로 자동형변환해서 b2저장한다
		
		System.out.println(b2);
		
		int a1;
		
		a1 = 10;
		
		System.out.println(a1);
		
	
		byte a = 127;
		System.out.println(a);
		a++;
		System.out.println(a);
		a++;
		System.out.println(a);
		
		
		int x = 10, y = 20;
	    System.out.println("x + y = " + (x + y));

		
	}

}
