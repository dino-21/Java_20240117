package ex01;

public class Hello {
	public static void main(String[] args) {

		int kor = 10;   //변수 선언
	
		
		kor = kor + 20;
		
		kor = kor + 100;
		
		System.out.println(kor);
		
		
		
		
		
		
		
		float f = 0;
		for(int i=0; i<30; i++)
			
			f = f + 0.1f;
		
		System.out.println(f);
		
			
		
		
	
	}
}


