package ex03;

public class Exam01 {

	public static void main(String[] args) {
		
		int exam  = 4000;         
		
		int hour = exam/3600;
		int minute = (exam % 3600)/60;
		int second = (exam % 3600)%60;
		
		System.out.println(hour + "시간 " + minute+"분 " + second+"초"  );
		
		//출력
		// 1시간 4분 0초

		
		int a = 5;
		int b = 0;
		int c = 0;
		int d = 0;
		int e = 0;
		
		b = a++;  
		/*
		 * b = a;
		 * a++;
		 */
		
		c = ++a;
		/*
		 * ++a;
		 * c = a
		 */
		
		d = ++c;
		e = b++;
		
		System.out.println("a : " + a);
		System.out.println("b : " + b);
		System.out.println("c : " + c);
		System.out.println("d : " + d);
		System.out.println("e : " + e);
		
		
		int x = 1;
		int y = 2;
		int z = ++x + -y;
		
		double f = 1.5;
		
		System.out.println("출력 결과 : " + z);
	
		System.out.println();
		System.out.print("afaf");
		System.out.print("afaf");
		
		char x2 = 65;
		
		System.out.println();
		System.out.println(f);
		System.out.println(x2);
		
		System.out.printf("x = %d\t y = %d\t z = %d\t f = %.3f\t x2 = %c \n", x,y,z, f,x2);
		
		long l  = 100;
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
